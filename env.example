# Server Configuration
NODE_ENV=development
PORT=3000

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/trading_agent
REDIS_URL=redis://localhost:6379

# Google AI Configuration
GOOGLE_API_KEY=your_google_api_key
GEMINI_API_KEY=your_gemini_api_key
GOOGLE_AGENT_PROJECT_ID=your_google_agent_project_id
GOOGLE_AGENT_LOCATION=us-central1

# API Keys
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key
FINNHUB_API_KEY=your_finnhub_api_key
TWITTER_BEARER_TOKEN=your_twitter_bearer_token
REDDIT_CLIENT_ID=your_reddit_client_id
REDDIT_CLIENT_SECRET=your_reddit_client_secret
REDDIT_USER_AGENT=your_reddit_user_agent

# Twilio Configuration (SMS Notifications)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number

# Trading Configuration
DEFAULT_WATCHLIST=AAPL,MSFT,GOOGL,TSLA,NVDA,AMZN,META,AMD,NFLX,CRM
ANALYSIS_INTERVAL=300000
SENTIMENT_UPDATE_INTERVAL=600000
OPTIONS_FLOW_UPDATE_INTERVAL=300000
TECHNICAL_UPDATE_INTERVAL=60000

# AI Model Configuration
GEMINI_MODEL=gemini-pro
GEMINI_MAX_TOKENS=8192
GEMINI_TEMPERATURE=0.7
AGENT_MEMORY_SIZE=1000

# Logging
LOG_LEVEL=info
LOG_FILE=logs/trading_agent.log

# Security
JWT_SECRET=your_jwt_secret_key
BCRYPT_ROUNDS=12

# Rate Limiting
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX_REQUESTS=100

# Cloud Configuration (for deployment)
CLOUD_PROVIDER=aws
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key 